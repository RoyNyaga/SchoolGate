<div id="school-main-div">
  <h1>Subjects</h1>
  <div id="subjects">
    <% @subjects.each do |subject| %>
      <%= render partial: "subjects/subject", locals: { subject: subject, is_admin_dashboard: true } %>
      <p>
        <%= link_to "Show this subject", subject %>
      </p>
    <% end %>
  </div>
  <%= link_to "New subject", new_subject_path %>
</div>

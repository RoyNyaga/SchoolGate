<div class="container p-0">
  <h1 class="text-center text-white py-5">Schools</h1>
  <div id="schools" class="section-div custom-border">
    <% @schools.each do |school| %>
      <%= render school %>
      <p>
        <%= link_to "Show this school", school %>
      </p>
    <% end %>
  </div>
  <%= link_to "New school", new_school_path %>
</div>

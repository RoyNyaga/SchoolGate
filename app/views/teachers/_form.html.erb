<%= form_with(model: teacher) do |form| %>
  <% if teacher.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(teacher.errors.count, "error") %> prohibited this teacher from being saved:</h2>
      <ul>
        <% teacher.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div class="row">
    <div class="col-md-6 my-2">
      <%= form.label :first_name, style: "display: block" %>
      <%= form.text_field :first_name, class: "general-input-styles", required: true %>
    </div>
    <div class="col-md-6 my-2">
      <%= form.label :last_name, style: "display: block" %>
      <%= form.text_field :last_name, class: "general-input-styles", required: true %>
    </div>
    <div class="col-md-6 my-2">
      <%= form.label :phone_number, style: "display: block" %>
      <%= form.number_field :phone_number, class: "general-input-styles", required: true %>
    </div>
    <div class="col-md-6 my-2">
      <%= form.label :town, style: "display: block" %>
      <%= form.text_field :town, class: "general-input-styles", required: true %>
    </div>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
